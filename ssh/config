Host *
    ServerAliveInterval 1200
    ServerAliveCountMax 12
    IdentitiesOnly yes
    TCPKeepAlive yes
    Compression yes

# Suita
Host gw-suita
    HostName        gw-suita.akashisn.info
    ProxyCommand    cloudflared access ssh --hostname %h
    User            admin
    ForwardAgent    yes
    IdentityFile    ~/.ssh/gpg.pub

