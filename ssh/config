Host *
    ServerAliveInterval 1200
    ServerAliveCountMax 12
    IdentitiesOnly yes
    TCPKeepAlive yes
    Compression yes

# Suita
Host gw-suita
    HostName        gw-suita.akashisn.info
    ProxyCommand    cloudflared access ssh --hostname %h
    User            admin
    ForwardAgent    yes
    IdentityFile    ~/.ssh/gpg.pub

Host ix2215-suita
    HostName        172.16.0.1
    User            nishi
    HostKeyAlgorithms ssh-rsa
    PubkeyAcceptedAlgorithms +ssh-rsa

Host er-x-suita
    HostName        172.16.0.2
    User            nishi
    IdentityFile    ~/.ssh/gpg.pub

Host truenas-suita
    HostName        172.16.10.10
    User            admin
    ForwardAgent    yes
    IdentityFile    ~/.ssh/gpg.pub

