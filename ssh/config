# Match
Match host 172.16.0.*,172.16.10.* !Exec "ping -c1 -t1 172.16.0.1"
    ProxyJump gw-suita

Match host 172.16.100.*,172.16.254.* !Exec "ping -c1 -t1 172.16.100.1"
    ProxyJump gw-tokyo
